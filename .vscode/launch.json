{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Eleventy",
      "program": "${workspaceFolder}/node_modules/.bin/eleventy",
      "args": ["--serve", "--quiet", "--config=.eleventy.config.gemini.js"],
      "console": "integratedTerminal",
      // "env": {
      //   "DEBUG": "*"
      // },
      "skipFiles": [
        "<node_internals>/**",
        "**/node_modules/**"
  ]
    }
  ]
}